{% extends 'basee.html.twig' %}

{% block title %}CadreMedicale
{% endblock %}

{% block body %}
	<h1>CadreMedicale</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ cadre_medicale.id }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ cadre_medicale.nom }}</td>
			</tr>
			<tr>
				<th>Prenom</th>
				<td>{{ cadre_medicale.prenom }}</td>
			</tr>
			<tr>
				<th>Service</th>
				<td>{{ cadre_medicale.service }}</td>
			</tr>
		</tbody>
	</table>

	<div class="btn-group ml-3 float-right mb-2" role="groupe">
		<a href="{{ path('cadre_medicale_index') }}">
			<button class="btn btn-outline-secondary">Back</button>
		</a>
		<div class="ml-2 mr-2">

			{{ include('cadre_medicale/_delete_form.html.twig') }}
		</div>
		<a href="{{ path('cadre_medicale_edit', {'id': cadre_medicale.id}) }}">
			<button class="btn btn-outline-secondary">edit</button>
		</a>
	{% endblock %}
