{% extends 'base.html.twig' %}

{% block title %}Service
{% endblock %}

{% block body %}
	<h1>Service</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ service.id }}</td>
			</tr>
			<tr>
				<th>Nom</th>
				<td>{{ service.nom }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('service_index') }}">
		<button type="button" class="btn btn-outline-secondary">back to list</button>
	</a>

	<a href="{{ path('service_edit', {'id': service.id}) }}">
		<button type="button" class="btn btn-outline-secondary">edit</button>
	</a>

	{{ include('service/_delete_form.html.twig') }}
{% endblock %}
